{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "antlr4",
            "type": "antlr-debug",
            "request": "launch",
            //"input": "${workspaceFolder}/${command:AskForTestInput}",
            "input": "input_macro.sas",
            "grammar": "sas_parser.g4",
            "startRule": "parse",
            "printParseTree": true,
            "visualParseTree": true
            // "visualParseTree": false
        }
    ]
}